{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "usuarios.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}